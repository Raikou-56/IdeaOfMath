@{
    ViewData[index: "Title"] = "CheckAnswer Page";
}

@model AnswerHistory

<p class="tst">@Model.StudentId @Model.SolvedAt.ToString(format: "yyyy/MM/dd")</p>

@if (Model.CloudinaryUrls == null || Model.CloudinaryUrls.Count == 0)
{
    <p class="tst">解答画像がありません。</p>
}
else
{
    @foreach (string url in Model.CloudinaryUrls)
    {
        <img src="@url" alt="解答画像" style="max-width:200px; margin:10px;" />
    }
}<br>

<p>@Model.Score</p>
